concrete HelloEnglish of Hello = {
	lincat Greeting, Recipient = {s: Str};

	lin
		Hello recip = {s = "hello" ++ recip.s};
		Goodbye recip = {s = "goodbye" ++ recip.s};
		World = {s = "world"};
		Mum = {s = "mum"};
		Friends = {s = "friends"};
		Cat = {s = "kitty"};
		Dog = {s = "dog"};
		RobotOverlords = {s = "robot overlords"};
}
