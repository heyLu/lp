concrete HelloGerman of Hello = {
	lincat Greeting, Recipient = {s: Str};

	lin
		Hello recip = {s = "hallo" ++ recip.s};
		Goodbye recip = {s = "auf wiedersehen" ++ recip.s};
		World = {s = "Welt"};
		Mum = {s = "Mama"};
		Friends = {s = "Freunde"};
		Cat = {s = "Kitty"};
		Dog = {s = "Hund"};
		RobotOverlords = {s = "erlauchte Herren Roboter"};
}
